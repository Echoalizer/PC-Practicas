public class Main {

	public static void (String[] args) {
		Scanner scan = new Scanner(System.in);
		int n;
		
		
		System.out.print("Introducir valor n: ");
		n = scan.nextInt();
		double[][] matriz1 = new double[n][n];
		double[][] matriz2 = new double[n][n];
		double[][] matriz3 = new double[n][n];
	}
}
