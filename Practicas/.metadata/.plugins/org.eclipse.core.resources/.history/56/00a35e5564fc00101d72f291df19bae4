package swift;

public class Hilo extends Thread {
	
	private double[][] _A;
	private double[][] _B;
	private double[][] _C;
	private int _longitud;
	private int _fila;
	
	
	public Hilo(double[][] A, double[][] B, double[][] C, int longitud, int fila) {
		_A = A;
		_B = B;
		_C = C;
		_longitud = longitud;
		_fila = fila;
	}
 	
}
